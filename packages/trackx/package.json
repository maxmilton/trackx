{
  "name": "trackx",
  "version": "0.6.0",
  "description": "Simple JavaScript exception tracking",
  "repository": "maxmilton/trackx",
  "homepage": "https://docs.trackx.app",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "main": "default.js",
  "module": "default.mjs",
  "exports": {
    ".": {
      "import": "./default.mjs",
      "require": "./default.js",
      "types": "./default.d.ts"
    },
    "./compat": {
      "import": "./compat.mjs",
      "require": "./compat.js",
      "types": "./compat.d.ts"
    },
    "./extended": {
      "import": "./extended.mjs",
      "require": "./extended.js",
      "types": "./extended.d.ts"
    },
    "./lite": {
      "import": "./lite.mjs",
      "require": "./lite.js",
      "types": "./lite.d.ts"
    },
    "./modern": {
      "import": "./modern.mjs",
      "require": "./modern.js",
      "types": "./modern.d.ts"
    },
    "./node": {
      "import": "./node.mjs",
      "require": "./node.js",
      "types": "./node.d.ts"
    },
    "./plugins/details": {
      "import": "./plugins/details.mjs",
      "require": "./plugins/details.js",
      "types": "./plugins/details.d.ts"
    },
    "./plugins/details-node": {
      "import": "./plugins/details-node.mjs",
      "require": "./plugins/details-node.js",
      "types": "./plugins/details-node.d.ts"
    },
    "./plugins/browser": {
      "import": "./plugins/browser.mjs",
      "require": "./plugins/browser.js",
      "types": "./plugins/browser.d.ts"
    },
    "./types": "./types.ts",
    "./package.json": "./package.json"
  },
  "browser": "default.js",
  "types": "default.d.ts",
  "files": [
    "compat.*",
    "default.*",
    "extended.*",
    "lite.*",
    "modern.*",
    "node.*",
    "plugins",
    "types.ts"
  ],
  "keywords": [
    "crash-reporting",
    "error-handling",
    "error-log",
    "error-monitoring",
    "error-reporting",
    "exception-handler",
    "logging",
    "trackx-client"
  ],
  "scripts": {
    "build": "pnpm run prebuild && rollup --config",
    "dev": "rollup --config --watch",
    "prebuild": "rm -rf plugins compat.* default.* extended.* lite.* modern.* node.* || true"
  },
  "dependencies": {
    "@ungap/global-this": "^0.4.4"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "1.0.1",
    "@rollup/plugin-commonjs": "24.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "5.0.1",
    "@types/ungap__global-this": "0.3.1",
    "esbuild": "0.16.9",
    "rollup": "3.7.5",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-terser": "7.0.2",
    "trackx": ".",
    "typescript": "4.8.4"
  }
}
